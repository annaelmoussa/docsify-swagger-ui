  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FlippedCodes/docsify-swagger-ui.git"
  },
  "keywords": [
    "docsify",
    "docsify.js",
    "plugin",
    "embed",
    "better embed"
  ],
